{
  "name": "pm-web-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "precommit": "eslint . --fix && prettier . --write",
    "gen": "supabase gen types typescript --local > src/supabase/types.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.49.1",
    "@tanstack/vue-query": "5.66.11",
    "pinia": "3.0.1",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@iconify/vue": "4.3.0",
    "@tailwindcss/vite": "4.0.8",
    "@tanstack/vue-query-devtools": "5.66.11",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "22.13.5",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/tsconfig": "0.7.0",
    "daisyui": "5.0.0-beta.8",
    "eslint": "9.21.0",
    "eslint-plugin-vue": "9.32.0",
    "prettier": "3.5.2",
    "prettier-plugin-tailwindcss": "0.6.11",
    "supabase": "2.15.8",
    "tailwindcss": "4.0.8",
    "typescript": "~5.7.2",
    "vite": "6.2.0",
    "vue-tsc": "2.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "supabase"
    ]
  }
}
